<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.isdb">

  <uses-permission android:name="android.permission.INTERNET" />
  <application
      android:allowBackup="true"
      android:icon="@mipmap/isdb_launcher"
      android:label="@string/app_name"
      android:networkSecurityConfig="@xml/security"
      android:roundIcon="@mipmap/ic_launcher_round"
      android:supportsRtl="true"
      android:theme="@style/MyMaterialTheme">
    <activity
        android:name="com.isdb.ui.login.LoginActivity"
        android:exported="true"
        android:label="@string/title_activity_login" />
    <activity android:name="com.isdb.activity.RegisterActivity" />
    <activity android:name="com.isdb.activity.LoginActivity" />
    <activity android:name="com.isdb.activity.HomeScreenActivity" />
    <activity android:name="com.isdb.activity.LoginMainActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
  </application>

</manifest>