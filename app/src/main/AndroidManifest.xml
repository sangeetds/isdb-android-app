<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.isdb">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/isdb_launcher"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/security"
        android:supportsRtl="true"
        android:theme="@style/MyMaterialTheme">
        <activity android:name="com.example.isdb.activity.RegisterActivity" />
        <activity android:name="com.example.isdb.activity.LoginActivity" />
        <activity android:name="com.example.isdb.activity.HomeScreenActivity" >
        </activity>
        <activity android:name="com.example.isdb.activity.LoginMainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>